#include <iostream>
int main()
{
	using std::cout;
	cout << 2 + 2 << '\n' << "hello";

	cout << '\n' << '\n';
	double i = 20 + 2;
	cout << i << '\n';
	i = i * 3;
	cout << i << '\n';
	std::cin >> i;
	double x = i * 2;
	cout << x;
}

