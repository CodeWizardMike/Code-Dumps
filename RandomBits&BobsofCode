#include <iostream>
using std::cout;
using std::cin;

int main()
{

	double numbertoguess;
	double numberyourguessing;
	double yes;
	double no;
	double replay;
	yes = 1;
	no = 0;


	cout << "player 1 type number player 2 has to guess: ";
	cin >> numbertoguess;
	cout << "player 2 guess the number now:";
	numberyourguessing = 0;
	replay = 0;
	
		while(replay = 1)
		{ 
			while (numberyourguessing != numbertoguess)
			{
				cin >> numberyourguessing;

				if (numberyourguessing > numbertoguess)
				{
					'\n';
					cout << "number is too big try smaller";
				}
				if (numberyourguessing < numbertoguess)
				{
					'\n';
					cout << "number is too small try bigger";
				}
				if (numberyourguessing == numbertoguess)
				{
					'\n';
					cout << "YOU WIN!";
				}
				if (numberyourguessing == numbertoguess)
				{
					'\n';
					cout << "Do you want to play again? (yes or no)";
						cin >> replay;
				}
			}
 }
}

